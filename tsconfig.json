{
  "compilerOptions": {
    "target": "es5",
    "module": "es2020",
    "outDir": "./build",
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "downlevelIteration": true
  }
}
